pipeline {
   agent any
   stages {
      stage('Starting the Release for Prod') {
		    steps {
			    xlrCreateRelease releaseTitle: 'Release for $BUILD_TAG', serverCredentials: 'XLReleaseServer', startRelease: true, template: 'Test/ProdReleaseTemplate', variables: [propertyName: 'BRANCH_NAME', propertyValue: '${BRANCH_NAME}']
			       }
     }        
  }
}
