pipeline {
   agent any
   stages {
      stage('Starting the Release for Prod') {
		    steps {
			    xlrCreateRelease releaseTitle: 'Release for $BUILD_TAG', serverCredentials: 'XLReleaseServer', startRelease: true, template: 'Test/ProdReleaseTemplate', variables: [[propertyName: 'BUILD_NUMBER', propertyValue: '${BUILD_NUMBER}'], [propertyName: 'RELEASE_BRANCH_NAME', propertyValue: '${RELEASE_BRANCH_NAME}']]
			       }
     }        
  }
}
